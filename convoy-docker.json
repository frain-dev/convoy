{
    "database": {
        "dsn": "mongodb://root:rootpassword@mongodb:27017"
    },
    "queue": {
        "type": "redis",
        "redis": {
            "dsn": "redis://redis_server:6379"
        }
    },
    "server": {
        "http": {
            "port": 5005
        }
    },
    "auth": {
        "type": "none"
    },
    "signature": {
        "header": "X-Company-Event-WebHook-Signature",
        "hash": "SHA256"
    },
    "strategy": {
        "type": "default",
        "default": {
            "intervalSeconds": 20,
            "retryLimit": 3
        }
    },
    "smtp": {
        "provider": "sendgrid",
        "url": "smtp.sendgrid.net",
        "port": 2525,
        "username": "apikey",
        "password": "<api-key-from-sendgrid>",
        "from": "support@frain.dev"
    },
    "ui": {},
    "disable_endpoint": false
}
