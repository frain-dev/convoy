{
    "host": "https://app.convoy.io",
    "multiple_tenants": false,
    "queue": {
        "type": "redis",
        "redis": {
            "dsn": "abc"
        }
    },
    "auth": {
        "file": {
            "basic": [
                {
                    "username": "test",
                    "password": "test",
                    "role": {
                        "type": "super_user"
                    }
                },
                {
                    "username": "test-group-filter",
                    "password": "test-group-filter",
                    "role": {
                        "groups": ["abcdef"],
                        "type": "super_user"
                    }
                }
            ]
        }
    },
    "server": {
        "http": {
            "port": 80
        }
    },
    "group": {
        "strategy": {
            "type": "default",
            "default": {
                "intervalSeconds": 125,
                "retryLimit": 15
            }
        },
        "signature": {
            "header": "X-Company-Event-WebHook-Signature",
            "hash": "SHA256"
        }
    }
}
