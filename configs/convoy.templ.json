{
    "database": {
        "type": "postgres",
        "dsn": "postgres://convoy:convoy@postgres/convoy?sslmode=disable&connect_timeout=30d"
    },
    "queue": {
        "type": "redis",
        "redis": {
            "dsn": "redis://redis_server:6379"
        }
    },
    "cache": {
        "type": "redis",
        "redis": {
            "dsn": "redis://redis_server:6379"
        }
    },
    "host": "{host}",
    "logger": {
        "type": "console",
        "server_log": {
            "level": "error"
        }
    },
    "smtp": {
        "provider": "sendgrid",
        "url": "smtp.sendgrid.net",
        "port": 2525,
        "username": "apikey",
        "password": "<api-key-from-sendgrid>",
        "from": "support@frain.dev"
    },
    "search": {
        "type": "typesense",
        "typesense": {
            "host": "http://typesense:8108",
            "api_key": "convoy"
        }
    },
    "server": {
        "http": {
            "ssl": false,
            "ssl_cert_file": "",
            "ssl_key_file": "",
            "port": 5005
        }
    },
    "auth": {
        "jwt": {
            "enabled": true
        },
        "native": {
            "enabled": true
        }
    }
}
