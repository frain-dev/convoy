{
    "database": {
        "dsn": "mongodb://inside-config-file"
    },
    "queue": {
        "type": "in-memory"
    },
    "server": {
        "http": {
            "port": 80
        }
    },
    "group": {
        "strategy": {
            "type": "default",
            "default": {
                "intervalSeconds": 125,
                "retryLimit": 15
            }
        },
        "signature": {
            "hash": "SHA256"
        }
    }
}
