{
  "auth": {
    "require_auth": true,
    "file": {
      "basic": [
        {
          "username": "123",
          "password": "abc",
          "role": {
            "type": "super_user"
          }
        }
      ]
    }
  },
  "database": {
    "dsn": "mongodb://inside-config-file"
  },
  "queue": {
    "type": "redis",
    "redis": {
      "dsn": "redis://localhost:8379"
    }
  },
  "server": {
    "http": {
      "port": 80
    }
  },
  "group": {
    "strategy": {
      "type": "exponential-backoff",
      "exponentialBackoff": {
        "retryLimit": 3,
        "backoff_times": [
          10,
          30,
          60,
          180
        ]
      }
    },
    "signature": {
      "hash": "SHA256"
    }
  }
}
