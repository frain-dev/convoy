{
    "database": {
        "dsn": "mongodb://inside-config-file"
    },
    "queue": {
        "type": "redis",
        "redis": {
            "dsn": "redis://localhost:8379"
        }
    },
    "server": {
        "http": {
            "port": 80
        }
    },
    "group": {
        "strategy": {
            "type": "exponential-backoff",
            "exponentialBackoff": {
                "retryLimit": 0
            }
        },
        "signature": {
            "hash": "SHA256"
        }
    }
}
