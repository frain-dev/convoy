{
  "auth": {
    "require_auth": true,
    "file": {
      "basic": [
        {
          "username": "123",
          "password": "abc",
          "role": {
            "type": "super_user"
          }
        }
      ]
    }
  },
  "database": {
    "dsn": "mongodb://inside-config-file"
  },
  "queue": {
    "type": "redis",
    "redis": {
      "dsn": "redis://localhost:8379"
    }
  },
  "server": {
    "http": {
      "port": 80
    }
  },
  "max_response_size": 51200,
  "group": {
    "strategy": {
      "type": "default",
      "default": {
        "intervalSeconds": 125,
        "retryLimit": 15
      }
    },
    "signature": {
      "header": "X-Convoy-Signature",
      "hash": "SHA256"
    }
  }
}
