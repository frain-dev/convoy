{
    "database": {
        "dsn": "postgres://inside-config-file"
    },
    "redis": {
        "dsn": "redis://localhost:8379"
    },
    "server": {
        "http": {
            "port": 8080
        }
    },
    "group": {
        "strategy": {
            "type": "default",
            "default": {
                "intervalSeconds": 125,
                "retryLimit": 15
            }
        },
        "signature": {
            "hash": "SHA256",
            "header": "x-test-signature"
        },
        "disable_endpoint": true,
        "replay_attacks": true
    },
    "auth": {
        "require_auth": true,
        "native": {
            "enabled": true
        },
        "file": {
            "basic": [
                {
                    "username": "admin",
                    "password": "qwerty",
                    "role": {
                        "type": "super_user",
                        "group": ""
                    }
                }
            ]
        }
    }
}
