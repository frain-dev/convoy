{
	"singleQuote": true,
	"tabWidth": 2,
	"printWidth": 80,
	"semi": true,
	"endOfLine": "auto",
	"arrowParens": "avoid",
	"bracketSpacing": true,
	"htmlWhitespaceSensitivity": "ignore",
	"insertPragma": false,
	"jsxSingleQuote": false,
	"proseWrap": "preserve",
	"quoteProps": "as-needed",
	"requirePragma": false,
	"trailingComma": "all",
	"useTabs": true,
	"vueIndentScriptAndStyle": false
}
