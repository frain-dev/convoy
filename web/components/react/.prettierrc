{
    "singleQuote": true,
    "tabWidth": 4,
    "printWidth": 210,
    "semi": true,
    "endOfLine": "auto",
    "arrowParens": "avoid",
    "bracketSpacing": true,
    "htmlWhitespaceSensitivity": "ignore",
    "insertPragma": false,
    "jsxBracketSameLine": true,
    "jsxSingleQuote": false,
    "proseWrap": "preserve",
    "quoteProps": "as-needed",
    "requirePragma": false,
    "trailingComma": "none",
    "useTabs": true,
    "vueIndentScriptAndStyle": false
}
