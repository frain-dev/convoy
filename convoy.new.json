{
  "database": {
    "dsn": "mongodb://root:rootpassword@mongodb:27017"
  },
  "queue": {
    "type": "redis",
    "redis": {
      "dsn": "redis://redis_server:6379"
    }
  },
  "server": {
    "http": {
      "port": 5005
    }
  },
  "auth": {
    "type": "none"
  },
  "strategy": {
    "type": "default",
    "default": {
      "intervalSeconds": 125,
      "retryLimit": 15
    }
  },
  "signature": {
    "header": "X-Company-Event-WebHook-Signature"
  }
}
